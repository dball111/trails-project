extends ../layout

block content
  script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")


  h1 #{title}

    div#transbox
        for places in result
              p
              label(for="location") Location:
              p#result #{places.city}, #{places.state}
              label(for="name") Name:
              p#result #{places.name}
              label(for="directions") Directions:
              p#result #{places.directions}
              label(for="lat") Latitude/Longitude:
              p#result #{places.lat}, #{places.lon}
              p
              hr

    

             
             
    
        script(src='/javascripts/script.js')


            







